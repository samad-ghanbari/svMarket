{
	"name": "svmarket",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"imports": {
		"#backend/*": "./backend/*"
	},
	"scripts": {
		"sv-dev": "vite dev",
		"ex-dev": "npx ts-node-dev index.mts",
		"build-svex": "vite build && npx ts-node index.mts",
		"dev": "concurrently \"vite dev\" \"npx ts-node-dev index.mts\"",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@types/express": "^5.0.5",
		"@types/node": "^22.18.13",
		"concurrently": "^9.2.1",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^5.4.0",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"flowbite-svelte": "^1.20.0",
		"heroicons-svelte": "^2.0.2",
		"mongoose": "^8.19.2"
	}
}
